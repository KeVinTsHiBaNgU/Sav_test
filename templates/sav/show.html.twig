{% extends 'base.html.twig' %}

{% block title %}Sav{% endblock %}

{% block body %}
    <h1>Sav</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sav.id }}</td>
            </tr>
            <tr>
                <th>Sav_nom</th>
                <td>{{ sav.savNom }}</td>
            </tr>
            <tr>
                <th>Sav_prenom</th>
                <td>{{ sav.savPrenom }}</td>
            </tr>
            <tr>
                <th>Sav_adr1</th>
