        </tbody>
    </table>

    <a href="{{ path('app_sav_index') }}" class="btn btn-secondary">retour</a>

    <a href="{{ path('app_sav_edit', {'id': sav.id}) }}" class="btn btn-primary">modifier</a>

    {{ include('sav/_delete_form.html.twig') }}
